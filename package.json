{
    "name": "gitfeed",
    "version": "0.1.0",
    "main": "index.ts",
    "packageManager": "pnpm@10.26.0",
    "engines": {
        "node": ">=24.0.0"
    },
    "scripts": {
        "prepare": "husky",
        "precommit": "lint-staged",
        "postinstall": "pnpm run generate",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watchAll",
        "start": "dotenv -c -- expo start",
        "build": "dotenv -c -- expo prebuild",
        "android": "dotenv -c -- expo run:android",
        "ios": "dotenv -c -- expo run:ios",
        "web": "dotenv -c -- expo start --web",
        "lint": "expo lint",
        "typecheck": "tsc --noEmit",
        "prettier": "prettier **/src/**/*.{ts,tsx} --check",
        "format": "concurrently pnpm:format:*",
        "format:eslint": "eslint --fix **/src/**/*.{tsx,ts,tsx}",
        "format:prettier": "prettier --write \"./**/*.{cjs,mjs,ts,tsx,scss,json}\"",
        "generate": "concurrently pnpm:generate:*"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        }
    },
    "lint-staged": {
        "./src/**/*.ts*": [
            "pnpm format:eslint",
            "pnpm format:prettier"
        ]
    },
    "dependencies": {
        "@bacons/text-decoder": "0.0.0",
        "@expo/vector-icons": "15.0.3",
        "@react-native-async-storage/async-storage": "2.2.0",
        "@react-navigation/bottom-tabs": "7.9.0",
        "@react-navigation/elements": "2.9.3",
        "@react-navigation/native": "7.1.26",
        "@shopify/flash-list": "2.2.0",
        "@tanstack/react-query": "5.90.12",
        "clsx": "2.1.1",
        "expo-application": "7.0.8",
        "expo-auth-session": "7.0.10",
        "expo-blur": "15.0.8",
        "expo-constants": "18.0.12",
        "expo-dev-client": "6.0.20",
        "expo-font": "14.0.10",
        "expo-haptics": "15.0.8",
        "expo-image": "3.0.11",
        "expo-linking": "8.0.10",
        "expo-localization": "17.0.8",
        "expo-router": "6.0.19",
        "expo-splash-screen": "31.0.12",
        "expo-status-bar": "3.0.9",
        "expo-symbols": "1.0.8",
        "expo-system-ui": "6.0.9",
        "expo-web-browser": "15.0.10",
        "expo": "54.0.29",
        "i18next": "25.7.3",
        "lodash": "4.17.21",
        "nativewind": "4.2.1",
        "react-dom": "19.0.0",
        "react-i18next": "16.5.0",
        "react-native-gesture-handler": "2.29.1",
        "react-native-keyboard-controller": "1.18.2",
        "react-native-reanimated": "3.17.4",
        "react-native-safe-area-context": "5.6.2",
        "react-native-screens": "4.19.0",
        "react-native-web": "0.21.2",
        "react-native-webview": "13.16.0",
        "react-native": "0.79.5",
        "react": "19.0.0",
        "tailwind-merge": "3.4.0",
        "tailwindcss": "3.4.17"
    },
    "devDependencies": {
        "@babel/core": "7.28.5",
        "@jest/globals": "30.2.0",
        "@testing-library/react-native": "13.1.1",
        "@types/jest": "29.5.14",
        "@types/lodash": "4.17.21",
        "@types/node": "24.10.4",
        "@types/react": "19.2.7",
        "babel-plugin-react-compiler": "19.1.0-rc.3",
        "babel-preset-expo": "54.0.8",
        "concurrently": "9.2.1",
        "dotenv-cli": "11.0.0",
        "eslint-config-expo": "10.0.0",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react-compiler": "19.1.0-rc.2",
        "eslint-plugin-unused-imports": "4.3.0",
        "eslint": "9.39.2",
        "expo-module-scripts": "5.0.8",
        "get-tsconfig": "4.13.0",
        "husky": "9.1.7",
        "jest-expo": "54.0.16",
        "jest": "29.7.0",
        "lint-staged": "16.2.7",
        "prettier-eslint": "16.4.2",
        "prettier-plugin-tailwindcss": "0.7.2",
        "prettier": "3.7.4",
        "react-native-svg-transformer": "1.5.2",
        "swagger-typescript-api": "13.2.16",
        "ts-node": "10.9.2",
        "typescript": "5.9.3"
    }
}
